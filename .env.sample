# Node Environment
NODE_ENV=development
PORT=5000

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_PROJECT_ID=your_project_id

# Receipt Parsing Configuration
# Choose one: claude, groq, or veryfi
# If not set, the system will auto-detect based on available API keys
RECEIPT_PARSER_PROVIDER=claude

# Claude Vision (Anthropic) - Recommended for high accuracy
ANTHROPIC_API_KEY=your_anthropic_api_key
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Groq (Llama Vision) - Fast and cost-effective
GROQ_API_KEY=your_groq_api_key
GROQ_MODEL=llama-3.2-90b-vision-preview

# Veryfi (OCR Service) - Specialized receipt parsing
VERYFI_CLIENT_ID=your_veryfi_client_id
VERYFI_CLIENT_SECRET=your_veryfi_client_secret
VERYFI_USERNAME=your_veryfi_username
VERYFI_API_KEY=your_veryfi_api_key
